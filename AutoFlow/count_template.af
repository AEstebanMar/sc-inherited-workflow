
run_count){
 resources: -c 6 -t 2-00:00:00 -m '50gb'
 module load cellranger/7.0.0
 ?
 cellranger count --id=$sample \
                  --sample=$sample \
                  --transcriptome=$count_transcriptome \
                  --fastqs=$read_path \
                  --localcores=[cpu] \
                  --localmem=50
 find -name "possorted_genome_bam.bam" -exec rm {} \;
}